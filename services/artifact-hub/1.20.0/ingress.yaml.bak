apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: artifact-hub-ingress
  namespace: ${releaseNamespace}
spec:
  rules:
    - host: artifacthub.${ingress_host}.sslip.io
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: artifact-hub-hub
                port:
                  number: 80
